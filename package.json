{
  "name": "@digitallinguistics/styles",
  "version": "0.17.0",
  "description": "A collection of DLx assets and styles",
  "homepage": "https://github.com/digitallinguistics/styles",
  "bugs": "https://github.com/digitallinguistics/styles/issues",
  "license": "MIT",
  "author": {
    "name": "Daniel W. Hieber",
    "url": "https://danielhieber.com"
  },
  "repository": "https://github.com/digitallinguistics/styles.git",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "npm run build-css && npm run build-docs",
    "build-css": "node build/buildCSS.js",
    "build-docs": "node build/clearDocs.js && npm run kss && node build/copyFiles.js",
    "kss": "./node_modules/.bin/kss --config kss.config.json",
    "upload": "node build/upload.js"
  },
  "devDependencies": {
    "@azure/storage-blob": "^10.5.0",
    "eslint": "^6.6.0",
    "feather-icons": "^4.28.0",
    "flexbox-reset": "^3.0.0",
    "fs-extra": "^9.0.1",
    "kss": "^3.0.1",
    "less": "^3.10.3",
    "less-plugin-clean-css": "^1.5.1",
    "michelangelo": "^0.8.0",
    "ora": "^4.0.4",
    "recursive-readdir": "^2.2.2"
  }
}
